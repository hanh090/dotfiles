{
  "coc.source.file.ignorePatterns": [
    "node_modules/*"
  ],
  "coc.preferences.floatActions": false,
  "eslint.options": {
    "ignorePattern": [
      "node_modules/*",
      "*.bs.js"
    ]
  },
  "eslint.run": "onSave",
  "java.jdt.ls.vmargs": "-javaagent:/Users/hanhle/.m2/repository/org/projectlombok/lombok/1.18.22/lombok-1.18.22.jar",
  "java.maven.downloadSources": true,
  "java.completion.filteredTypes": [
    "com.sun.*",
    "antlr.*"
  ],
  "solargraph.autoformat": true,
  "solargraph.useBundler": false,
  "solargraph.checkGemVersion": false,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "sql.formatOptions": {
    "keywordCase": "upper"
  },
  "suggest.timeout": 6000,
  "snippets.shortcut": "SP",
  "flutter.sdk.flutter-lookup": "asdf which flutter",
  "workspace.ignoredFiletypes": [
    "log",
    "txt",
    "help",
    "class",
    "jar"
  ],
  "yank.highlight.enable": false
}
